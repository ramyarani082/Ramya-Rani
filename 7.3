def find_words(words):
    keyboard = {
        'q': 1, 'w': 1, 'e': 1, 'r': 1, 't': 1, 'y': 1, 'u': 1, 'i': 1, 'o': 1, 'p': 1,
        'a': 2, 's': 2, 'd': 2, 'f': 2, 'g': 2, 'h': 2, 'j': 2, 'k': 2, 'l': 2,
        'z': 3, 'x': 3, 'c': 3, 'v': 3, 'b': 3, 'n': 3, 'm': 3
    }

    result = []
    for word in words:
        row = set(keyboard[c] for c in word.lower() if c in keyboard)
        if len(row) == 1:
            result.append(word)

    return result
words=[]
n=int(input())
for i in range(n):
    st=input()
    words.append(st)
word=find_words(words)
if(len(word)==0):
    print("No words")
for i in range(len(word)):

    print(word[i])
